extend ../templates/layout-scroll.pug

block content

	include ../templates/includes/title-card.pug

	include article.pug

	.main-content-inner-wrapper(data-content-field="main-content")
		.gallery-wrapper(data-controller="GalleryLayout")
			.slides
				.grid-wrapper

					+article('Deep Recurrent Dialogue Generation'
							, 'May' 
							, '22'
							, '2012'
							, '../assets/images/g-home-1.png'
							, 'In this section we consider the problem of random walk in three dimensions, and show that unlike a random walk in 2D that returns to the origin infinitely often, a random walk in 3D diverges almost surely.'
							) 


					+article('Random Walk in 3D'
							, 'July'
							, '34'
							, '2013'
							, '../assets/images/rand-walk-1.png'
							, 'In this section we consider the problem of random walk in three dimensions, and show that unlike a random walk in 2D that returns to the origin infinitely often, a random walk in 3D diverges almost surely.'
							)

